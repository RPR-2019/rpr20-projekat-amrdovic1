<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.RegisterController">
  <stylesheets>
      <URL value="@register.css" />
  </stylesheets>
    <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="ALWAYS" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Email:" />
      <Label text="Username:" GridPane.rowIndex="2" />
      <Label text="Password:" GridPane.rowIndex="8" />
      <Label text="Repeat password:" GridPane.rowIndex="10" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="12">
        <buttons>
          <Button fx:id="cancel" cancelButton="true" mnemonicParsing="false" onAction="#cancelClick" text="Cancel" />
            <Button fx:id="proceed" defaultButton="true" mnemonicParsing="false" onAction="#proceedClick" text="Proceed" />
        </buttons>
      </ButtonBar>
      <Label text="Gender:" GridPane.rowIndex="4" />
      <Label text="Date of birth:" GridPane.rowIndex="6" />
      <TextField fx:id="email" GridPane.columnIndex="1" />
      <TextField fx:id="username" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <ButtonBar fx:id="genders" prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
         <buttons>
            <RadioButton fx:id="male" mnemonicParsing="false" text="Male" />
            <RadioButton fx:id="female" mnemonicParsing="false" text="Female" />
         </buttons>
      </ButtonBar>
      <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="8" />
      <PasswordField fx:id="repeatPassword" GridPane.columnIndex="1" GridPane.rowIndex="10" />
      <Label prefHeight="17.0" prefWidth="45.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label prefHeight="17.0" prefWidth="66.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Label fx:id="gendersError" textFill="RED" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="5" />
      <Label prefHeight="17.0" prefWidth="90.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />
      <Label prefHeight="17.0" prefWidth="71.0" GridPane.columnIndex="1" GridPane.rowIndex="9" />
      <Label fx:id="repeatPasswordError" textFill="RED" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="11" />
      <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
         <children>
            <ChoiceBox fx:id="selectDay" prefHeight="25.0" prefWidth="53.0" />
            <ChoiceBox fx:id="selectMonth" prefHeight="25.0" prefWidth="59.0" />
            <ChoiceBox fx:id="selectYear" prefHeight="25.0" prefWidth="105.0" />
         </children>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
